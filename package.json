{
  "name": "rabrpc",
  "version": "0.1.12",
  "description": "RabbitMQ RPC Library based on Rabbot",
  "main": "index.js",
  "scripts": {
    "preversion": "npm test",
    "test": "snyk test && eslint . && npm run mocha",
    "mocha": "npm run test-unit && npm run test-integration",
    "test-unit": "mocha \"test/bootstrap.test.js\" \"test/unit/**/*.test.js\"",
    "test-unit-watch": "mocha --watch \"test/bootstrap.test.js\" \"test/unit/**/*.test.js\"",
    "test-integration": "mocha \"test/bootstrap.test.js\" \"test/integration/**/*.test.js\"",
    "coverage": "istanbul cover _mocha \"test/bootstrap.test.js\" \"test/unit/**/*.test.js\"",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "keywords": [
    "rabbitmq",
    "microservice",
    "pubsub",
    "reqres",
    "rpc"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:swarthy/rabrpc.git"
  },
  "author": "Alexander Mochalin <horroshow@mail.ru>",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.0",
    "debug": "^2.6.3",
    "jsend": "^1.0.2",
    "lodash": "^4.17.4",
    "rabbot": "^1.1.0",
    "snyk": "^1.25.2",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "eslint": "^3.18.0",
    "eslint-config-inclusive": "^1.0.3",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "sinon": "^1",
    "sinon-as-promised": "^4.0.2",
    "sinon-chai": "^2.8.0"
  },
  "snyk": true
}
